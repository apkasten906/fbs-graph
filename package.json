{
  "name": "fbs-graph-graphql",
  "version": "0.3.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "score": "tsx scripts/compute-leverage.ts",
    "import:csv": "tsx scripts/import-from-csv.ts",
    "preview:playoff": "tsx scripts/run-playoff-preview.ts",
    "web:serve": "tsx scripts/serve-web.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest --run --reporter=json",
    "lint": "eslint .",
    "format": "prettier --write .",
    "audit:data": "npx tsx ./scripts/audit-data.ts",
    "fetch:confs": "tsx scripts/fetch-cfbd-conferences-to-json.ts",
    "fetch:teams": "tsx scripts/fetch-cfbd-teams-to-csv.ts",
    "fetch:schedules": "tsx scripts/fetch-cfbd-schedules-to-csv.ts",
    "seasons:make": "tsx scripts/generate-team-seasons-from-teams.ts",
    "fetch:rankings": "tsx scripts/fetch-cfbd-rankings-to-polls.ts",
    "fetch:elo": "tsx scripts/fetch-cfbd-elo-to-seasons.ts",
    "fetch:sp": "tsx scripts/fetch-cfbd-sp-to-seasons.ts",
    "fetch:ratings": "npm run fetch:elo && npm run fetch:sp",
    "fetch:all-ranks": "npm run fetch:rankings && npm run fetch:ratings",
    "fetch:ranks": "npm run fetch:all-ranks",
    "fetch:all": "npm run fetch:confs && npm run fetch:teams && npm run seasons:make && npm run fetch:schedules && npm run fetch:ranks",
    "validate:data": "tsx scripts/validate-data-ids.ts",
    "setup": "npm run validate:data && powershell.exe -ExecutionPolicy Bypass -File .\\scripts\\setup.ps1",
    "generate:static": "tsx scripts/generate-static-data.ts",
    "build:pages": "npm run generate:static && tsx scripts/build-pages.ts",
    "generate:playoff-page": "tsx scripts/generate-playoff-page.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "csv-parse": "^5.5.6",
    "dotenv": "^17.2.3",
    "git-filter-repo": "^0.0.30",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^15.5.4",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.6.2",
    "tsx": "^4.16.2",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
